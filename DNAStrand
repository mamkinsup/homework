function DNAStrand(dna){
    let myString = []
for (i=0; i<=dna.length; i++) {
if (dna[i]==='A') {
 let a = dna[i].replace(/A/,'T');
 myString.push(a)
}
if (dna[i] === 'T') {
let b = dna[i].replace(/T/,'A');
myString.push(b)
}
if (dna[i] === 'G') {
let c = dna[i].replace(/G/,'C'); 
myString.push(c)
}
if (dna[i] === 'C') {
let p = dna[i].replace(/C/,'G');
myString.push(p)
}
}
let newString = myString.toString()
return (newString.replace(/,/g,''))
//your code here
}